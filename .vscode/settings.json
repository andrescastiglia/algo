{
  "editor.rulers": [
    120
  ],
  "editor.formatOnSave": true,
  "search.exclude": {
    "**/.git": true,
    "**/target": true,
    "**/tmp": true
  },
  "[rust]": {
    "editor.defaultFormatter": "rust-lang.rust-analyzer"
  },
  "rust-analyzer.cargo.noDefaultFeatures": true,
  "rust-analyzer.cargo.features": [
    "binance",
    "bitstamp"
  ]
}